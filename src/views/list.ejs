<!DOCTYPE html>
<html lang="pt-br">

<%- include('partials/head.ejs') %>

<body>

    <h1><%= title %></h1>

    <%- include('partials/navbar.ejs') %>
    
    <ul>
        <% users.forEach((user) => { %>
            <li>
                <%= user.name %> - <%= user.email %> - <%= user.age %> -
                <a href="/edit?id=<%= user._id %>">[editar]</a> - <!--RECBE POR QUERY STRING-->
                <a href="/remove/<%= user._id %>">[remover]</a> <!-- MAIS COMUM-->
            </li>
        <% }); %>
    </ul>    

    <%- include('partials/foot.ejs') %>

</body>
</html>